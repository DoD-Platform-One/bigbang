{
  "name": "repo-sync",
  "author": "Snekcode",
  "homepage": "https://repo1.dso.mil/platform-one/big-bang/apps/product-tools/repo-sync",
  "version": "1.0.0",
  "license": "ISC",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp ./.env ./lib/.env",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"npm start\"",
    "start": "ts-node ./src/index.ts",
    "prod-start": "npm run build && node ./lib/index.js",
    "lint": "eslint --ext .ts .",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "smee:start": "source ./.env && smee -u https://smee.io/$SMEE -p $PORT -P /Repo-Sync",
    "smee:ssl": "source ./.env && smee -u https://smee.io/$SMEE -p $PORT -t https://localhost/repo-sync",
    "smee:record": "source ./.env && smee -u https://smee.io/$SMEE -p $PORT -P /record"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql-request": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "msw": "^1.2.2",
    "serverless-http": "^3.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "nodemon": "^3.0.0",
    "smee-client": "^1.2.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
