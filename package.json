{
  "name": "repo-sync",
  "author": "Snekcode",
  "homepage": "https://repo1.dso.mil/platform-one/big-bang/apps/product-tools/repo-sync",
  "version": "1.0.0",
  "license": "ISC",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"npm start\"",
    "start": "ts-node ./src/index.ts",
    "lint": "eslint --ext .ts .",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "smee:start": "source ./.env && smee -u https://smee.io/$SMEE -p $PORT -P /Repo-Sync",
    "smee:record": "source ./.env && smee -u https://smee.io/$SMEE -p $PORT -P /record"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "serverless-http": "^3.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^23.3.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^10.17.60",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "nodemon": "^1.19.4",
    "prettier": "^2.8.8",
    "smee-client": "^1.2.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
