{
  "name": "RepoSync",
  "author": "Snekcode",
  "homepage": "https://repo1.dso.mil/platform-one/big-bang/apps/product-tools/repo-sync",
  "version": "1.0.0",
  "license": "ISC",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"npm start\"",
    "start": "ts-node ./src/index.ts",
    "lint": "standard **/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "serverless-http": "^3.1.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "@types/supertest": "^2.0.12",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^29.5.0",
    "nodemon": "^1.17.2",
    "standard": "^10.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  }
}
